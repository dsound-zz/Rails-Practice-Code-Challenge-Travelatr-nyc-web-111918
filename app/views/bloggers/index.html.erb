
<h1>Bloggers</h1>

<%= button_to 'Create Blogger', new_blogger_path, method: "get",class: "alert button" %>

<div class="progress [radius round]">
  <span class="meter" style="width:70%;">
</span>
</div>

<ul class="side-nav">

<% @bloggers.each do |blogger|  %>
<p><%= link_to blogger.name, blogger_path(blogger), class: "active" %></p>
<% end %>
</ul>
