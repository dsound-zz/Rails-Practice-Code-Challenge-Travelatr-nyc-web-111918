

<h2>Post about <%= link_to @post.destination.name_country, destination_path(@post.destination) %> </h2>

<p>Title: <%= @post.title %></p>
<p>Conent: <%= @post.content %></p>
<p>Post likes: <%= @post.likes %></p>
<p><%=  button_to 'like', like_path(@post), :method => "patch" %></p>
<p><%= @post.likes %>
<p>Post author <%= link_to @post.blogger.name, blogger_path(@post.blogger) %></p><br>
